<launch>

  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen" clear_params="true">
    <param name="base_local_planner" value="teb_local_planner/TebLocalPlannerROS" />
    <param name="base_global_planner" value="global_planner/GlobalPlanner" />
    <!-- <param name="base_global_planner" value="astar_planner/AstarPlanner" /> -->
    <rosparam file="$(find ucar_navigation)/config/move_base_f/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find ucar_navigation)/config/move_base_f/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find ucar_navigation)/config/move_base_f/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find ucar_navigation)/config/move_base_f/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find ucar_navigation)/config/move_base_f/prohibition_areas.yaml" command="load" ns="global_costmap/costmap_prohibition_layer" />
    <!-- <rosparam file="$(find ucar_navigation)/config/move_base_f/prohibition_areas.yaml" command="load" ns="local_costmap/costmap_prohibition_layer" /> -->

    <rosparam file="$(find ucar_navigation)/config/move_base_f/move_base_params.yaml" command="load" />
    <rosparam file="$(find ucar_navigation)/config/move_base_f/teb_local_planner_params.yaml" command="load" />
    <rosparam file="$(find ucar_navigation)/config/move_base_f/GlobalPlanner.yaml" command="load" />
    <!-- <rosparam file="$(find ucar_navigation)/config/move_base/astar_param.yaml" command="load" /> -->
    
  </node>
  
</launch>



